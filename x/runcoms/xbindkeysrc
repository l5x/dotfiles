#Delete
"xvkbd -xsendevent -text '\[Delete]'"
    m:0x8 + c:22
    Alt + BackSpace

#End
"xvkbd -xsendevent -text '\[End]'"
    m:0x8 + c:114
    Alt + Right

#Home
"xvkbd -xsendevent -text '\[Home]'"
    m:0x8 + c:113
    Alt + Left

#Page Down
"xvkbd -xsendevent -text '\[Page_Down]'"
    m:0x8 + c:116
    Alt + Down

#Page Up
"xvkbd -xsendevent -text '\[Page_Up]'"
    m:0x8 + c:111
    Alt + Up

#Delete
"xvkbd -xsendevent -text '\[Delete]'"
    m:0x4 + c:22
    Control + BackSpace

